{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "ES2017",
      "DOM"
    ],
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "allowUnreachableCode": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node"
  },
  "include": [
    "src"
  ]
}
